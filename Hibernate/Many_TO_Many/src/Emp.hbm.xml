<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
<class name="mypack.Emp" table="empdata">
<id name="eid" type="int">
<column name="emp_id"></column>
<generator class="assigned"></generator>
</id>

<property name="ename"></property>

<list name="projects" table="emp_project" fetch="select" cascade="all">
<key column="emp_id"></key>
<index column="type1"></index>
<many-to-many class="mypack.Project" column="project_id"></many-to-many>
</list>
</class>
</hibernate-mapping>